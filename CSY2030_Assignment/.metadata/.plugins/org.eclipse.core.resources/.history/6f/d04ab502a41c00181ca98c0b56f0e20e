	public static void add_Car_list(){
		try {

			FileInputStream fis = new FileInputStream("vehicle.dat");
			ObjectInputStream ois = new ObjectInputStream(fis);
		    Car obj=null;
			
			while ((obj=(Car)ois.readObject())!=null) {
					
				vehicle_list.add(obj);
			


				
			}
			
			 ois.close();
			 
			} catch (EOFException ex) { //This exception will be caught when EOF is reached
			 System.out.println("End of file reached.");
			 } catch (ClassNotFoundException ex) {
			 ex.printStackTrace();
			 } catch (FileNotFoundException ex) {
			 ex.printStackTrace();
			 } catch (IOException ex) {
			 ex.printStackTrace();
			 } 
		
	}
	public static void read_Car_first(){
		try {

			FileInputStream fis = new FileInputStream("vehicle.dat");
			ObjectInputStream ois = new ObjectInputStream(fis);
		    Car obj=null;
			
			while ((obj=(Car)ois.readObject())!=null) {

				System.out.println("Reg No Of Car "+obj.Reg_number);
				System.out.println("Make Of Car "+obj.Make_Vehicle);
				System.out.println("Model Of Car "+obj.Model_Vehicle);
				System.out.println("Hire Rate Of Car "+obj.Hire_Rate_Vehicle);
				System.out.println("Speed Of Car "+obj.Speed_Vehicle);
				System.out.println("Number of Doors Of Car "+obj.Number_doors);
				System.out.println("Fuel Type Of Car "+obj.Fuel_Type);
				System.out.println("Type Of Vehicle "+obj.Vehicle_type);
				System.out.println("Status Of Car "+obj.Vehicle_status);


				
			}
			
			 ois.close();
			 
			} catch (EOFException ex) { //This exception will be caught when EOF is reached
			 System.out.println("End of file reached.");
			 } catch (ClassNotFoundException ex) {
			 ex.printStackTrace();
			 } catch (FileNotFoundException ex) {
			 ex.printStackTrace();
			 } catch (IOException ex) {
			 ex.printStackTrace();
			 } 
	}
	
	public static void read_miniBus_first(){
		try {
			//Display the details for specific vehicle
			FileInputStream fis = new FileInputStream("minibus.dat");
			ObjectInputStream ois = new ObjectInputStream(fis);
		    MiniBus obj=null;
			
			while ((obj=(MiniBus)ois.readObject())!=null) {
			
				
					
				System.out.println("Reg No Of MiniBus "+obj.Reg_number);
				System.out.println("Make Of MiniBus "+obj.Make_Vehicle);
				System.out.println("Model Of MiniBus "+obj.Model_Vehicle);
				System.out.println("Hire Rate Of MiniBus "+obj.Hire_Rate_Vehicle);
				System.out.println("Speed Of MiniBus "+obj.Speed_Vehicle);
				System.out.println("Number of Doors Of MiniBus "+obj.Number_doors);
				System.out.println("Type Of Vehicle "+obj.Vehicle_type);
				System.out.println("Status Of MiniBus "+obj.Vehicle_status);

				
				if(obj.Hire_Rate_Vehicle==10.09){
					System.out.println("Yes");
					System.out.println("Type Of Vehicle "+obj.Vehicle_type);

				}
				
				
			}
			 ois.close();
			} catch (EOFException ex) { //This exception will be caught when EOF is reached
			 System.out.println("End of file reached.");
			 } catch (ClassNotFoundException ex) {
			 ex.printStackTrace();
			 } catch (FileNotFoundException ex) {
			 ex.printStackTrace();
			 } catch (IOException ex) {
			 ex.printStackTrace();
			 } 

		
	}
	public static void add_miniBus_list(){
		
		try {

			FileInputStream fis = new FileInputStream("minibus.dat");
			ObjectInputStream ois = new ObjectInputStream(fis);
		    MiniBus mini_obj=null;
			
			while ((mini_obj=(MiniBus)ois.readObject())!=null) {
			
				
					
				minibus_list.add(mini_obj);

				
				
	
				
			}
			
			 ois.close();
			 
			} catch (EOFException ex) { //This exception will be caught when EOF is reached
			 System.out.println("End of file reached.");
			 } catch (ClassNotFoundException ex) {
			 ex.printStackTrace();
			 } catch (FileNotFoundException ex) {
			 ex.printStackTrace();
			 } catch (IOException ex) {
			 ex.printStackTrace();
			 } 
	}
	
public static  void  Remove_Previous_Panel(){
		
		//Removing Panel
		main_panel.removeAll();
		main_panel.repaint();
		main_panel.revalidate();
		
		//Adding new Panel
		main_panel.repaint();
		main_panel.revalidate();
		
	}
	
	
	

