					System.out.println("Seat Capacity Of MiniBus "+mini_obj.Seat_Capacity_MiniBus);
					System.out.println("Type Of MiniBus "+mini_obj.Vehicle_type);
					System.out.println("Status Of MiniBus "+mini_obj.Vehicle_status);

					
					
